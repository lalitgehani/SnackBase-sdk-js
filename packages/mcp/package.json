{
  "name": "@snackbase/mcp",
  "version": "0.1.0",
  "description": "MCP server for SnackBase",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "bin": {
    "snackbase-mcp": "./dist/index.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown src/index.ts --format esm --watch",
    "build": "tsdown src/index.ts --format esm --dts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.1",
    "@snackbase/sdk": "workspace:*"
  },
  "devDependencies": {
    "@snackbase/tsconfig": "workspace:*",
    "@types/node": "^20.11.0",
    "tsdown": "^0.20.0-beta.4",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
